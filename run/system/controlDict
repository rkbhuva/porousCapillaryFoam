FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     porousCapillaryFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         3600;

deltaT          1e-5;

//writeControl    runTime;

//writeControl    timeStep;

//writeInterval   100;
writeControl    adjustable;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

adjustableRunTime  yes;

maxCo           1;

maxDeltaT       1;



